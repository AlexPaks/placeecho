{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Frontend: dev (Vite)",
      "type": "shell",
      "command": "npm run dev",
      "options": {
        "cwd": "${workspaceFolder}/Frontend"
      },
      "isBackground": true,
      "problemMatcher": {
        "background": {
          "activeOnStart": true,
          "beginsPattern": "vite\\s+\\d+\\.\\d+\\.\\d+\\s+ready\\s+in",
          "endsPattern": "Local:\\s+http://localhost:5173"
        }
      }
    }
  ]
}
